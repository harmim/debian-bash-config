[user]
	name = Foo Bar
	email = foo@bar.com
#	signingkey = ...
[alias]
	logd = log --decorate --source --graph --all
	pulls = ! git stash -q && git pull && git stash pop -q
[push]
	default = simple
[pull]
	rebase = true
[diff]
	compactionHeuristic = true
[core]
	eol = lf
	editor = vim
	autocrlf = input
	excludesfile = ~/.gitcfg/.gitignore_global
	commentChar = ";"
